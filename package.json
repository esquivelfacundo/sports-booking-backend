{
  "name": "sports-booking-backend",
  "version": "1.0.0",
  "description": "Backend API for Sports Booking Platform",
  "main": "src/app.js",
  "scripts": {
    "start": "node scripts/migrate-and-start.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "db:init": "node src/scripts/initDatabase.js",
    "db:sync": "node -e \"require('./src/models').sequelize.sync({ alter: true }).then(() => console.log('âœ… DB synced')).catch(console.error).finally(() => process.exit())\"",
    "db:seed": "node src/scripts/seedTestData.js",
    "db:seed:complete": "node src/scripts/seedCompleteData.js"
  },
  "keywords": [
    "sports",
    "booking",
    "api",
    "nodejs",
    "express"
  ],
  "author": "Sports Booking Platform",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "json2csv": "^5.0.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.0.4",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.104.0",
